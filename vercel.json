{
  "version": 2,
  "builds": [
    {
      "src": "multi_modal_flight_map.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "multi_modal_flight_map:server"
    }
  ]
}
